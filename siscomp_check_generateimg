import sqlite3
import base64

con = sqlite3.connect('edgedb')
c = con.cursor()

query = c.execute('SELECT picture FROM imagebank')
data = c.fetchone()
print(type(data[0]))

imagedata = data[0]
print(type(data[0]), data[0])
thing = base64.b64decode(imagedata)
print(type(imagedata))
image_result = open('jpgsymb.jpg', 'wb')
image_result.write(imagedata)

